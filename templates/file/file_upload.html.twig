{% extends 'base.html.twig' %}

{% block title %}Save File{% endblock %}

{% block body %}
  {{ include("navbar.html.twig") }}
  <div class="w3-third w3-container">
    <form action="{{ path('file_upload') }}" method="POST" class="w3-border w3-panel" enctype="multipart/form-data">
      <p>
        <label>Filename</label>
        <br>
        <input type="text" id="filename" name="filename" class="db-input"/>
      </p>
      <p>

      </p>
      <p>
        <label for="file">Select File</label>
        <br>
        <input type="file" id="file" name="file"/>
      </p>
      <p>
        <button type="submit" class="db-button">Upload File</button>
      </p>
    </form>
  </div>

{% endblock %}
